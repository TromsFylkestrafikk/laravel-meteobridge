{
    "name": "tromsfylkestrafikk/laravel-meteobridge",
    "description": "Feed laravel with weather data from meteobridge pws devices.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "laravel",
        "pws",
        "personal weather station",
        "meteobridge"
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/broadcasting": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TromsFylkestrafikk\\Meteobridge\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TromsFylkestrafikk\\Meteobridge\\MeteobridgeServiceProvider"
            ]
        }
    },
    "authors": [
        {
